<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="700.0" prefWidth="800.0" style="-fx-background-color: #877171;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.tv.HomeController">
    <children>
      <ImageView fx:id="gif" fitHeight="701.0" fitWidth="803.0" pickOnBounds="true">
      </ImageView>
        <Label fx:id="welcomeText" alignment="CENTER" layoutX="220.0" layoutY="314.0" textFill="WHITE">
            <font>
                <Font size="39.0" />
            </font>
        </Label>
        <Button fx:id="startButton" layoutX="369.0" layoutY="396.0" mnemonicParsing="false" onAction="#turnOff" text="Turn off tv" />
      <Button fx:id="channelButton" layoutX="372.0" layoutY="454.0" mnemonicParsing="false" onAction="#toChannel" text="Channels" />
    </children>
</AnchorPane>
